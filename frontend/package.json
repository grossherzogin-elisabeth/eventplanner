        "@vue/eslint-config-typescript": "^14.6.0",
        "autoprefixer": "^10.4.21",
        "eslint": "^9.35.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-prettier": "^5.5.4",
        "eslint-plugin-vue": "^10.4.0",
        "postcss": "^8.5.6",
        "prettier": "^3.6.2",
        "prettier-plugin-tailwindcss": "^0.6.14",
        "tailwindcss": "^3.4.17",
        "typescript": "^5.9.2",
        "typescript-eslint": "^8.43.0",
        "vite": "^7.1.5",
