        "svg-country-flags": "^1.2.10",
        "uuid": "^11.1.0",
        "vue": "^3.5.13",
        "vue-draggable-next": "^2.2.1",
        "vue-i18n": "^11.1.1",
        "vue-router": "^4.5.0",
        "vuedraggable": "^4.1.0",
        "vuejs3-datepicker": "^1.0.19"
