        "check:lint": "npx eslint src",
        "check:typescript": "vue-tsc"
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "^7.0.1",
        "feiertagejs": "^1.4.1",
        "i18n-iso-countries": "^7.14.0",
        "i18n-nationality": "^1.4.0",
        "markdown-it": "^14.1.0",
        "svg-country-flags": "^1.2.10",
        "uuid": "^13.0.0",
        "vue": "^3.5.21",
        "vue-draggable-next": "^2.3.0",
        "vue-i18n": "^11.1.12",
        "vue-router": "^4.5.1",
        "vuedraggable": "^4.1.0",
        "vuejs3-datepicker": "^1.1.3"
    },
    "devDependencies": {
        "@babel/core": "^7.28.4",
        "@babel/preset-env": "^7.28.3",
        "@babel/preset-typescript": "^7.27.1",
